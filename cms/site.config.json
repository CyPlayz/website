  "private": {
    "log4js": {
      "appenders": {
        "console": {
          "type": "console"
        },
        "everything": {
          "type": "file",
          "filename": "logs/app.log"
        },
        "emergencies": {
          "type": "file",
          "filename": "logs/error.log"
        },
        "just-errors": {
          "type": "logLevelFilter",
          "appender": "emergencies",
          "level": "error"
        }
      },
      "categories": {
        "default": {
          "appenders": [
            "console",
            "everything",
            "just-errors"
          ],
          "level": "info"
        }
      }
    }
